{
  "name": "go",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "app",
  "workspaceFolder": "/go/src/app",
  "settings": { 
    "go.gopath": "/go",
    "go.inferGopath": true,
    "go.useLanguageServer": true
  },
  "extensions": [
    "ms-vscode.go",
    "golang.go"
  ],
  "shutdownAction": "stopCompose"
}